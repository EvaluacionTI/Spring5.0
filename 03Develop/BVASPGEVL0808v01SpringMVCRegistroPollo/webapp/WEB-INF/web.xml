<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Spring MVC - Registro de Pollo</display-name>

	<servlet>
		<servlet-name>springMVCKitchen</servlet-name>
		<servlet-class>org.springframework.web.servlet.DistpatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>springMVCKitchen</servlet-name>
		<url-pattern>*.pollo</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>pollomain.jsp</welcome-file>
	</welcome-file-list>
</web-app>