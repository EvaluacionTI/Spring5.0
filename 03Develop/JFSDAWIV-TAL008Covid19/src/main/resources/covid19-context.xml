<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			 "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="idCEVirusCovid19" 	class="pe.unjfsc.daw.spring5.entity.CEVirusCovid19">
		<property name="dayProyectado" value="3"></property>
		<property name="numeroContagio" value="56"></property>
		<property name="numeroFallecido" value="15000"></property>
